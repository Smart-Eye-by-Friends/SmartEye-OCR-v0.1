# SmartEye v0.1 - 개발 환경 변수 설정
# 개발 환경용 설정

# Spring Boot 설정
SPRING_PROFILES_ACTIVE=dev

# OpenAI API 키 (개발용 더미 키)
OPENAI_API_KEY=dummy-dev-key

# 데이터베이스 설정 (개발용)
DB_NAME=smarteye_dev
DB_USERNAME=dev
DB_PASSWORD=dev
DB_PORT=5433

# Redis 설정 (개발용)
REDIS_PORT=6380

# LAM 마이크로서비스 설정 (개발용 - 리소스 절약)
LAM_WORKERS=2
LAM_MEMORY_LIMIT=2G
LAM_CPU_LIMIT=1.0
LAM_MEMORY_RESERVATION=1G
LAM_CPU_RESERVATION=0.5

# 개발용 보안 설정
JWT_SECRET=dev-jwt-secret
ENCRYPTION_KEY=dev-encryption-key

# 로그 레벨 (개발용)
LOG_LEVEL=DEBUG

# 외부 서비스 URL
LAM_SERVICE_URL=http://smarteye-lam-dev:8081
